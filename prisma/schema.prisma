// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Recipes {
  id          Int    @id @unique @default(autoincrement())
  email       String
  name        String
  ingredients Json
  preparation String
  diffculty   String
  duration    String
}
